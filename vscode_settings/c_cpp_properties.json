{
    "configurations": [
        {
            "name": "Linux",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "includePath": [
                "${workspaceFolder}/include",
                "/usr/include",
                "/usr/local/include",
                "/usr/include/libxml2"
            ],
            "defines": [
                "DEBUG",
                "LIBXML_TEST_VERSION"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/include",
                    "/usr/include",
                    "/usr/local/include",
                    "/usr/include/libxml2"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}
